    <!-- CSS Styles -->
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Poppins', sans-serif;
        }

        body {
            background-color: #f9f9f9;
            color: #333;
        }

        /* Navbar Styles */
        .navbar {
            background-color: #1a1a1a;
            padding: 20px 0;
            position: sticky;
            top: 0;
            z-index: 100;
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px 30px;
        }

        .navbar:hover {
            background-color: #333;
        }

        .logo {
            font-size: 24px;
            font-weight: 700;
            color: #ffd700;
            text-decoration: none;
        }

        .nav-links {
            display: flex;
            list-style-type: none;
        }

        .nav-links li {
            margin: 0 20px;
        }

        .nav-links a {
            color: white;
            text-decoration: none;
            font-size: 18px;
            transition: all 0.3s ease;
        }

        .nav-links a:hover {
            color: #ffd700;
            transform: translateY(-5px);
        }

        .hamburger {
            display: none;
            flex-direction: column;
            justify-content: space-between;
            width: 30px;
            height: 25px;
            cursor: pointer;
        }

        .hamburger div {
            background-color: white;
            height: 4px;
            width: 100%;
            border-radius: 5px;
        }

        /* Header Section Styles */
        .header {
            background-color: #c8e6c9; /* Light Green */
            background-size: cover;
            background-position: center;
            height: 100vh;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            color: #000000;
            padding: 0 20px;
        }

        .header-content h1 {
            font-size: 60px;
            font-weight: 700;
            margin-bottom: 20px;
            animation: fadeIn 1.5s ease-in;
        }

        .header-content p {
            font-size: 20px;
            margin-bottom: 30px;
            max-width: 600px;
            margin-left: auto;
            margin-right: auto;
        }

        .header-content .btn {
            padding: 12px 30px;
            background-color: #ffd700;
            color: #333;
            font-size: 18px;
            text-decoration: none;
            border-radius: 5px;
            transition: all 0.3s ease;
        }

        .header-content .btn:hover {
            background-color: #ff8c00;
            transform: translateY(-5px);
        }

        /* Features Section Styles */
        .features {
            background-color: #e4f2e3;
            padding: 50px 20px;
            text-align: center;
        }

        .feature-box {
            display: flex;
            justify-content: center;
            gap: 20px;
            margin-top: 30px;
        }

        .feature {
            background-color: #fff;
            padding: 40px;
            border-radius: 10px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
            width: 30%;
            text-align: center;
            transition: transform 0.3s ease;
        }

        .feature:hover {
            transform: translateY(-10px);
        }

        .feature i {
            font-size: 50px;
            color: #ffd700;
            margin-bottom: 20px;
        }

        .feature h3 {
            font-size: 24px;
            margin-bottom: 20px;
        }

        /* Contact Form Styles */
        .contact-section {
            background-color: #cdeef2;
            padding: 50px 20px;
            text-align: center;
            margin-top: 50px;
        }

        .contact-section h2 {
            font-size: 36px;
            margin-bottom: 30px;
        }

        .contact-form {
            max-width: 600px;
            margin: 0 auto;
            background-color: #f4f4f4;
            padding: 30px;
            border-radius: 10px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
        }

        .contact-form input,
        .contact-form textarea {
            width: 100%;
            padding: 12px;
            margin-bottom: 15px;
            border-radius: 5px;
            border: 1px solid #ddd;
            font-size: 16px;
        }

        .contact-form button {
            padding: 12px 30px;
            background-color: #ffd700;
            color: #333;
            border: none;
            border-radius: 5px;
            font-size: 18px;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .contact-form button:hover {
            background-color: #ff8c00;
            transform: translateY(-5px);
        }

        /* Footer Section Styles */
        .footer {
            background-color: #1a1a1a;
            color: #fff;
            padding: 20px;
            text-align: center;
        }

        .footer p {
            font-size: 16px;
        }

        .footer a {
            color: #ffd700;
            text-decoration: none;
        }

        .social-icons {
            margin-top: 20px;
        }

        .social-icons a {
            color: #ffd700;
            font-size: 24px;
            margin: 0 10px;
            text-decoration: none;
            transition: color 0.3s ease;
        }

        .social-icons a:hover {
            color: #ff8c00;
        }

        /* Boxed Section Styles */
        .boxed-section {
            background-color: #fff;
            padding: 40px 20px;
            margin-top: 50px;
            border-radius: 10px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
            max-width: 1200px;
            margin-left: auto;
            margin-right: auto;
            text-align: center;
        }

        .boxed-section h2 {
            font-size: 36px;
            margin-bottom: 20px;
            font-weight: bold;
        }

        .boxed-section p {
            font-size: 20px;
            margin-bottom: 30px;
            line-height: 1.6;
        }

        .boxed-section .btn {
            padding: 12px 30px;
            background-color: #ffd700;
            color: #333;
            font-size: 18px;
            text-decoration: none;
            border-radius: 5px;
            transition: all 0.3s ease;
        }

        .boxed-section .btn:hover {
            background-color: #ff8c00;
            transform: translateY(-5px);
        }

        /* Animations */
        @keyframes fadeIn {
            0% { opacity: 0; }
            100% { opacity: 1; }
        }

        /* Media Queries for Responsiveness */
        @media (max-width: 768px) {
            .header-content h1 {
                font-size: 40px;
            }

            .header-content p {
                font-size: 16px;
            }

            .feature-box {
                flex-direction: column;
                gap: 30px;
            }

            .feature {
                width: 80%;
                margin: 0 auto;
            }

            .navbar {
                flex-direction: column;
                align-items: flex-start;
            }

            .nav-links {
                display: none;
                flex-direction: column;
                width: 100%;
            }

            .nav-links li {
                margin: 10px 0;
                width: 100%;
                text-align: left;
            }

            .hamburger {
                display: flex;
            }

            .nav-links.active {
                display: flex;
            }
        }

        @media (max-width: 480px) {
            .header-content h1 {
                font-size: 30px;
            }

            .header-content p {
                font-size: 14px;
            }

            .contact-form {
                padding: 20px;
            }

            .boxed-section h2 {
                font-size: 28px;
            }

            .boxed-section p {
                font-size: 16px;
            }
        }
    </style>
</head>
<body>

